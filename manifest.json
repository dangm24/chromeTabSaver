{
  "manifest_version": 2,

  "name": "chromeTabSaver",
  "description": "Extension to save currently open tabs and reopen them all at once",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}